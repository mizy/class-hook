{
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": {
                    "version": 3
                },
                "useBuiltIns": "usage",
                "targets": {
                    "ie": 9,
                    "chrome": 52,
                    "safari": 10,
                    "opera": 32,
                    "firefox": 30
                }
            }
        ],
        "@babel/preset-react"
	],
    "plugins": ["@babel/plugin-proposal-class-properties"]
}
